{

  "name": "BookmarkShareTool",
  "description": "This is app to share bookmarks in team",
  "version": "1.0",
  "permissions": [
        "bookmarks",
        "http://localhost:8080/*"
        ],

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Bookmarks"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com; object-src 'self'"
}